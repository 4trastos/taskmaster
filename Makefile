# Makefile for TaskMaster C++ Project

CXX = g++
CXXFLAGS = -Wall -Wextra -Werror -std=c++17 -g
LDFLAGS = -pthread

# Directories
SRC_DIR = src
INCL_DIR = incl
OBJ_DIR = obj

# Target
TARGET = taskmaster

# Source files
SRCS = main.cpp \
       ConfigParser.cpp \
       Program.cpp \
       ProcessManager.cpp \
       SignalHandler.cpp \
       TaskmasterShell.cpp \
       Logger.cpp \
       utils.cpp

AUX_SRCS = parse_utils.cpp

# Object files
OBJS = $(SRCS:%.cpp=$(OBJ_DIR)/$(SRC_DIR)/%.o) \
       $(AUX_SRCS:%.cpp=$(OBJ_DIR)/aux/%.o)

# Include paths
INCLUDES = -I$(INCL_DIR)

# Colors for output
RED = \033[0;31m
GREEN = \033[0;32m
YELLOW = \033[0;33m
BLUE = \033[0;34m
CYAN = \033[0;36m
MAGENTA = \033[0;35m
NC = \033[0m # No Color

# ASCII Art
define SUCCESS_ART
$(GREEN)
